<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vtradex.wms.server.model.organization.WmsUserSupplier" table="WMS_USER_SUPPLIER">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">wseq_UserSupplier</param>
                <param name="parameters">START WITH 1</param>
            </generator>
        </id>
        <discriminator column="DISCRIMINATOR" type="string"/>

		<many-to-one name="userHead" class="com.vtradex.wms.server.model.organization.WmsUserSupplierHead">
			<column name="USERHEAD_ID" not-null="true" unique-key="UK_USER_SUPPLIER"/>
		</many-to-one>
		<many-to-one name="supplier" class="com.vtradex.wms.server.model.organization.WmsOrganization">
			<column name="SUPPLIER_ID" not-null="true" unique-key="UK_USER_SUPPLIER"/>
		</many-to-one>
		
		<component name="updateInfo" class="com.vtradex.thorn.server.model.UpdateInfo">
			<property name="creatorId" type="long" column="CREATOR_ID"/>
			<property name="creator" type="string" length="50" column="CREATOR"/>
			<property name="createdTime" type="timestamp" column="CREATED_TIME"/>
			<property name="lastOperatorId" type="long" column="LAST_OPERATOR_ID"/>
			<property name="lastOperator" type="string" length="50" column="LAST_OPERATOR"/>
			<property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
		</component>
	 </class>
</hibernate-mapping>