<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vtradex.wms.server.model.middle.MiddleCompanyExtends" table="MIDDLE_COMPANY_EXTENDS">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">wseq_middleCompanyExtends</param>
                <param name="parameters">START WITH 1</param>
            </generator>
        </id>
        <discriminator column="DISCRIMINATOR" type="string"/>
        
        <property name="sender" type="string">
			<column name="SENDER" not-null="true" length="50" unique-key="UK_MIDDLE_COMPANY_EXTENDS"/>
		</property>
		<many-to-one name="warehouse" class="com.vtradex.wms.server.model.warehouse.WmsWarehouse">
			<column name="WAREHOUSE_ID" not-null="true"/>
		</many-to-one>
		<many-to-one name="company" class="com.vtradex.wms.server.model.organization.WmsOrganization">
			<column name="COMPANY_ID" not-null="true"/>
		</many-to-one>
		<property name="description" column="DESCRIPTION" type="string" length="255"/>
		<component name="updateInfo" class="com.vtradex.thorn.server.model.UpdateInfo">
			<property name="creatorId" type="long" column="CREATOR_ID"/>
			<property name="creator" type="string" length="50" column="CREATOR"/>
			<property name="createdTime" type="timestamp" column="CREATED_TIME"/>
			<property name="lastOperatorId" type="long" column="LAST_OPERATOR_ID"/>
			<property name="lastOperator" type="string" length="50" column="LAST_OPERATOR"/>
			<property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
		</component>
		
	</class>
</hibernate-mapping>