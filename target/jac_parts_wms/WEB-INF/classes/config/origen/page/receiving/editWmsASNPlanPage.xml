<?xml version="1.0" encoding="UTF-8"?>

<pages>
    <editPage id="editWmsASNPlanPage" title="editWmsASNPlanPage" entityClass="com.vtradex.wms.server.model.receiving.WmsBooking" width="500" height="400" onClose="refreshParent">
        <workflow/>
        <initListeners/>
        <inputUIs>
            <hidden id="booking.id" title="booking.id" reserve="false"/>
			<text id="booking.code" title="booking.code" row="1" col="1"  span="1" readOnly="true" required="false" reserve="false" forceOverride="true" focusUI="false" inVisible="false" trimSpace="true">
                <visibleExpression/>
                <hql/>
                <eventListeners/>
            </text>
            <remote id="booking.dock.id" title="booking.dock.id" row="2" col="1" span="1" readOnly="false" required="true" reserve="false" forceOverride="true" focusUI="false" inVisible="false" multiple="false" manualEditable="true" displayColumn="3" percentSignPosition="both" displayedTableHead="序号,代码">
                <visibleExpression/>
                <hql><![CDATA[SELECT
dock.id,
dock.code
FROM WmsDock dock
WHERE 1=1
AND dock.code LIKE :param 
AND dock.status = 'ENABLED'
AND dock.beReceive = true
AND dock.warehouseArea.warehouse.id = #{SESSION_WAREHOUSE}]]></hql>
                <eventListeners/>
            </remote>
            <remote id="booking.asn.id" title="booking.asn.id" row="2" col="2" span="1" readOnly="false" required="true" reserve="false" forceOverride="true" focusUI="false" inVisible="false" multiple="false" manualEditable="true" displayColumn="2" percentSignPosition="both" displayedTableHead="序号,代码,货主">
                <visibleExpression/>
                <hql><![CDATA[SELECT
asn.id,
asn.code,
company.name								
FROM WmsASN asn
left join asn.company company
WHERE 1=1
AND asn.code LIKE :param 
AND (asn.status = 'OPEN' or asn.status = 'ACTIVE')
AND (select count(*) from WmsASNDetail detail where detail.asn.id = asn.id and detail.booking.id is not null) = 0
AND asn.warehouse.id = #{SESSION_WAREHOUSE}
order by asn.id desc]]></hql>
                <eventListeners/>
            </remote>
            <text id="booking.planType" title="booking.planType" row="2" col="2" defaultValue="收货" span="1" readOnly="false" required="false" reserve="false" forceOverride="true" focusUI="false" inVisible="true" trimSpace="true">
                <visibleExpression/>
                <hql/>
                <eventListeners/>
            </text>
            <date id="booking.asnPlannedStartTime" title="booking.asnPlannedStartTime" row="3" col="1" span="1" readOnly="false" required="true" reserve="false" forceOverride="true" focusUI="false" inVisible="false" showTime="true" defaultCurrentDate="false">
                <visibleExpression/>
                <hql/>
                <eventListeners/>
            </date>
            <date id="booking.asnPlannedEndTime" title="booking.asnPlannedEndTime" row="3" col="2" span="1" readOnly="false" required="true" reserve="false" forceOverride="true" focusUI="false" inVisible="false" showTime="true" defaultCurrentDate="false">
                <visibleExpression/>
                <hql/>
                <eventListeners/>
            </date>
            <textArea id="booking.remark" title="booking.remark" row="4" col="1" span="2" width="335" readOnly="false" required="false" reserve="false" forceOverride="true" focusUI="false" inVisible="false">
                <visibleExpression/>
                <hql/>
                <eventListeners/>
            </textArea>
        </inputUIs>
    </editPage>
</pages>
