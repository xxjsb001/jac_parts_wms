                    <visibleExpression/>
                    <hql><![CDATA[ SELECT
 billType.id,
 billType.code,
 billType.name
 FROM WmsBillType billType
 WHERE 1=1 
 AND billType.type = 'SHIP'					
 AND (billType.code LIKE :param OR billType.name LIKE :param)
 AND billType.company.id = ${pickTicket.company.id}
 AND billType.status = 'ENABLED']]></hql>
                    <eventListeners/>
