                    <visibleExpression/>
                    <hql><![CDATA[SELECT 
company.id,
company.code,
company.name
FROM WmsOrganization company
WHERE company.beCompany = true
AND company.status = 'ENABLED'
AND (company.code LIKE :param OR company.name LIKE :param)
 		]]></hql>
                    <eventListeners/>
