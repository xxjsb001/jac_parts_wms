	        <column id="apl.supplierName" title="apl.supplierName" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
	        <column id="apl.packageNo" title="apl.packageNo" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
	        <column id="apl.packageName" title="apl.packageName" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
	        <column id="apl.packageNum" title="apl.packageNum" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
	        <column id="apl.partNo" title="apl.partNo" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
