<?xml version="1.0" encoding="UTF-8"?>

<pages>
    <maintainPage id="maintainWmsPickTicketAndStationPage" title="maintainWmsPickTicketAndStationPage" autoQuery="true" entityClass="com.vtradex.wms.server.model.shipping.WmsMoveDocAndStation">
        <workflow/>
        <datasource><![CDATA[SELECT was.id,
md.code,
item.code,
item.name,
was.quantity,
was.loadage,
was.locationCode,
was.routeNo,
ptd.pickWorker,
ptd.pickWorkerCode 
 from WmsMoveDocAndStation was 
left join was.item item 
left join was.moveDocDetail ptd
left join ptd.moveDoc md 
where 1=1 order by was.id desc]]></datasource>
        <columns>
            <column id="was.id" title="was.id" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="long"/>
