package com.vtradex.wms.server.model.warehouse;

import com.vtradex.thorn.server.model.Entity;

public class WmsWorkArea extends Entity{

	private static final long serialVersionUID = -646975712324028524L;
	/**
	 * 所属库区
	 */
    private WmsWarehouseArea warehouseArea;
    /**
     * 工作区代码
     */
    private String code;
    /**
     * 状态
     */
    private String status;
    /**
     * 描述
     */
    private String description;
    
	public WmsWarehouseArea getWarehouseArea() {
		return warehouseArea;
	}
	public void setWarehouseArea(WmsWarehouseArea warehouseArea) {
		this.warehouseArea = warehouseArea;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
    
}
 