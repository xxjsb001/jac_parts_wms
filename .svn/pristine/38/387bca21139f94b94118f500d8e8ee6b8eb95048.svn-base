$承运商指派 = 查表("R101_基础数据_承运商指派规则表",省,城市)
如果($承运商指派 == 空) {
    $承运商指派=查表("R101_基础数据_承运商指派规则表",$省,"所有")
}
如果($承运商指派 == 空) {
    $承运商指派 = 查表("R101_基础数据_承运商指派规则表","所有","所有")
}


$发货月台 = 查表("R101_基础数据_发货月台规则表",$承运商指派.承运商)
如果($发货月台 == 空){
	$发货月台 = 查表("R101_基础数据_发货月台规则表","所有")
}


如果($承运商指派 != 空){
	返回结果.承运商 = $承运商指派.承运商
}
如果($发货月台 != 空){
	返回结果.发货月台 = $发货月台.发货月台
}
