<?xml version="1.0" encoding="UTF-8"?>

<pages>
    <maintainPage id="maintainWmsMasterBOLPage" title="maintainWmsMasterBOLPage" autoQuery="true" entityClass="com.vtradex.wms.server.model.shipping.WmsMasterBOL">
        <workflow/>
        <datasource><![CDATA[SELECT masterBOL.id,
masterBOL.code,
masterBOL.carrier.name,
masterBOL.updateInfo.createdTime,
masterBOL.status,
masterBOL.packageQuantity,
masterBOL.scatteredQuantity,
masterBOL.weight,
masterBOL.volume,
masterBOL.customer.name,
masterBOL.shipToName,
masterBOL.driver,
masterBOL.vehicleNo,
masterBOL.intendShipDate,
masterBOL.shipTime,
masterBOL.description 
 FROM WmsMasterBOL masterBOL
 LEFT JOIN masterBOL.carrier 
 LEFT JOIN masterBOL.customer 
 WHERE 1=1
 AND masterBOL.warehouse = #{SESSION_WAREHOUSE} ORDER BY masterBOL.id DESC
 ]]></datasource>
        <columns>
            <column id="masterBOL.id" title="masterBOL.id" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="long"/>
            <column id="masterBOL.code" title="masterBOL.code" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.carrier.name" title="masterBOL.carrier.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.updateInfo.createdTime" title="masterBOL.updateInfo.createdTime" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="datetime"/>
            <column id="masterBOL.status" title="masterBOL.status" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="enum" format="enumFormat" formatParam="WmsMasterBOLStatus"/>
            <column id="masterBOL.packageQuantity" title="masterBOL.packageQuantity" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="masterBOL.scatteredQuantity" title="masterBOL.scatteredQuantity" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="masterBOL.weight" title="masterBOL.weight" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="masterBOL.volume" title="masterBOL.volume" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="masterBOL.customer.name" title="masterBOL.customer.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.shipToName" title="masterBOL.shipToName" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.driver" title="masterBOL.driver" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.vehicleNo" title="masterBOL.vehicleNo" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="masterBOL.intendShipDate" title="masterBOL.intendShipDate" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="datetime"/>
            <column id="masterBOL.shipTime" title="masterBOL.shipTime" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="datetime"/>
            <column id="masterBOL.description" title="masterBOL.description" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
        </columns>
    </maintainPage>
</pages>
