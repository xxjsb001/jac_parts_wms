package com.vtradex.wms.server.model.interfaces;

import com.vtradex.thorn.server.model.Entity;
/**
 * 间接获取W_SUPPLY_ITEM_ERP数据
 * 简称表B
 */
public class MiddleSupplyItemErpB extends Entity{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String company;//仓库
	
	private String itemCode;//物料代码
	
	private String invLoc;//寄存临采货主
	
	private String status;//状态	

	public MiddleSupplyItemErpB() {
		super();
	}

	public MiddleSupplyItemErpB(String company, String itemCode, String invLoc,
			String status) {
		super();
		this.company = company;
		this.itemCode = itemCode;
		this.invLoc = invLoc;
		this.status = status;
	}


	public String getCompany() {
		return company;
	}

	public void setCompany(String company) {
		this.company = company;
	}
	public String getItemCode() {
		return itemCode;
	}

	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}

	public String getInvLoc() {
		return invLoc;
	}


	public void setInvLoc(String invLoc) {
		this.invLoc = invLoc;
	}


	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

}
