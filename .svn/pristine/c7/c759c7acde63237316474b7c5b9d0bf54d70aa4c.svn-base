<?xml version="1.0" encoding="UTF-8"?>

<pages>
    <maintainPage id="maintainWmsMasterBOLDetailPage" title="maintainWmsMasterBOLDetailPage" autoQuery="false" entityClass="com.vtradex.wms.server.model.shipping.WmsBOLDetail">
        <workflow/>
        <datasource><![CDATA[
SELECT bolDetail.id,
bolDetail.bol.code,
bolDetail.subCode,
bolDetail.beReturn,
bolDetail.bol.status,
bolDetail.bol.billTypeName,
bolDetail.bol.vehicleNo,
bolDetail.bol.vehicle.license,
bolDetail.bol.driver,
bolDetail.bol.wmsDriver.name,
bolDetail.bol.shipTime,
bolDetail.task.workDoc.code,
bolDetail.task.workDoc.requireArriveDate,
bolDetail.itemKey.item.code,
bolDetail.itemKey.item.name,
bgy.name,
bolDetail.quantityBU,
bolDetail.slr,
bolDetail.tfd,
bolDetail.activeTime 
 FROM WmsBOLDetail bolDetail 
 LEFT JOIN bolDetail.task.workDoc workDoc
 LEFT JOIN bolDetail.task.moveDocDetail.moveDoc.pickTicket pickTicket 
 left join bolDetail.itemKey.item.worker bgy
 WHERE 1=1 
  AND bolDetail.bol.warehouse = #{SESSION_WAREHOUSE}
 ORDER BY bolDetail.bol.code desc,bolDetail.id 
 ]]></datasource>
        <columns>
         	<column id="bolDetail.id" title="bolDetail.id" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.code" title="bol.code" width="120" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.subCode" title="bol.subCode" width="120" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bolDetail.beReturn" title="bolDetail.beReturn" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="boolean" format="booleanFormat"/>
            <column id="bol.status" title="bol.status" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="enum" format="enumFormat" formatParam="WmsBOLStatus"/>
            <column id="bol.billTypeName" title="bol.billType.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.vehicleNo" title="bol.vehicleNo" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.vehicle.license" title="bol.vehicle.license" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.driver" title="bol.driver" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.wmsDriver.name" title="bol.wmsDriver.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bol.shipTime" title="bol.shipTime" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="date"/>
            <column id="workDoc.code" title="workDoc.code" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="workDoc.requireArriveDate" title="workDoc.requireArriveDate" width="140" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="datetime" format="dateFormat" formatParam="y-m-d h:m:s"/>
            <column id="bolDetail.item.code" title="bolDetail.item.code" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bolDetail.item.name" title="bolDetail.item.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="byg.name" title="item.workerName" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bolDetail.quantityBU" title="bolDetail.quantityBU" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="bolDetail.slr" title="bolDetail.slr" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
            <column id="bolDetail.tfd" title="bolDetail.tfd" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle"/>
        	<column id="bolDetail.activeTime" title="bolDetail.activeTime" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="datetime" format="dateFormat" formatParam="y-m-d h:m:s"/>
        </columns>
    </maintainPage>
</pages>
