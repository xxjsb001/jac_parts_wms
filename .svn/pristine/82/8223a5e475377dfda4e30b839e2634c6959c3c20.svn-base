<?xml version="1.0" encoding="UTF-8"?>

<pages>
    <maintainPage id="maintainWmsTaskAndStationPage" title="maintainWmsTaskAndStationPage" autoQuery="true" entityClass="com.vtradex.wms.server.model.shipping.WmsTaskAndStation">
        <workflow/>
        <datasource><![CDATA[SELECT wts.id,
moveDoc.code,
moveDocDetail.item.code,
moveDocDetail.item.name,
task.type,
task.originalBillCode,
task.relatedBill,
task.pallet,
task.planQuantity,
task.planQuantityBU,
task.movedQuantityBU,
task.tiredMovedQuantityBU,
wts.container
 from WmsTaskAndStation wts 
left join wts.task task 
left join task.moveDocDetail moveDocDetail
left join moveDocDetail.moveDoc moveDoc
where 1=1 order by wts.id desc]]></datasource>
        <columns>
            <column id="wts.id" title="wts.id" visible="false" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="moveDoc.code" title="moveDoc.code" visible="true" sortable="true" horizonAlign="left" verticalAlign="middle" dataType="string"/>
            <column id="moveDocDetail.item.code" title="item.code" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="moveDocDetail.item.name" title="item.name" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="task.type" title="task.type" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="enum" format="enumFormat" formatParam="WmsTaskType"/>
            <column id="task.originalBillCode" title="task.originalBillCode" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="task.relatedBill" title="task.relatedBill" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="task.pallet" title="task.pallet" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double"/>
            <column id="task.planQuantity" title="task.planQuantity" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="double" color="red" />
            <column id="task.planQuantityBU" title="task.planQuantityBU" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="task.movedQuantityBU" title="task.movedQuantityBU" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="task.tiredMovedQuantityBU" title="task.tiredMovedQuantityBU" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
            <column id="wts.container" title="wts.container" visible="true" sortable="true" horizonAlign="center" verticalAlign="middle" dataType="string"/>
	        
        </columns>
    </maintainPage>
</pages>
