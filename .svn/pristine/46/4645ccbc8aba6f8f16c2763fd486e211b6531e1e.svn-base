			return false;
		}
		if (this.getTrackExtendPropC20() && StringUtils.isEmpty(lotInfo.getExtendPropC20())) {
			return false;
		}
		return true;
	}

	private boolean requireLotInfo() {
		if (this.getTrackStorageDate() || this.getTrackProduceDate() || this.getTrackSOI() || this.getTrackSupplier() 
				|| this.getTrackExtendPropC1() || this.getTrackExtendPropC2() || this.getTrackExtendPropC3() || this.getTrackExtendPropC4()
				|| this.getTrackExtendPropC5() || this.getTrackExtendPropC6() || this.getTrackExtendPropC7()
				|| this.getTrackExtendPropC8() || this.getTrackExtendPropC9() || this.getTrackExtendPropC10()
				|| this.getTrackExtendPropC11() || this.getTrackExtendPropC12() || this.getTrackExtendPropC13() || this.getTrackExtendPropC14()
				|| this.getTrackExtendPropC15() || this.getTrackExtendPropC16() || this.getTrackExtendPropC17()
				|| this.getTrackExtendPropC18() || this.getTrackExtendPropC19() || this.getTrackExtendPropC20()) {
			return true;
		}
		return false;
	}

