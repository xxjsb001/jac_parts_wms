	public boolean verify(LotInfo lotInfo){
		if (!this.requireLotInfo()) {
			return true;
		}

		if (this.requireLotInfo() && lotInfo == null) {
			return false;
		}
		
		if (this.getTrackStorageDate() && lotInfo.getStorageDate() == null) {
			return false;
		}
		
		if (this.getTrackProduceDate() && lotInfo.getProductDate() == null) {
			return false;
		}
		
		if (this.getTrackSOI() && StringUtils.isEmpty(lotInfo.getSoi())) {
			return false;
		}
		if (this.getTrackSupplier() && lotInfo.getSupplier() == null) {
			return false;
		}
		if (this.getTrackExtendPropC1() && StringUtils.isEmpty(lotInfo.getExtendPropC1())) {
