package com.vtradex.wms.server.model.interfaces;

import com.vtradex.thorn.server.model.Entity;

/**
 * 直接获取W_SUPPLY_ITEM_ERP数据
 * 简称表A
 */
public class MiddleSupplyItemErpA extends Entity{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private Long tagId;//版本序号-暂时不用

	private String company;//仓库
	
	private String itemCode;//物料代码
	
	private String invLoc;//寄存临采货主
	
	private String status;//状态	

	public MiddleSupplyItemErpA() {
		super();
	}

	public MiddleSupplyItemErpA(String company, String itemCode, String invLoc,
			String status) {
		super();
		this.company = company;
		this.itemCode = itemCode;
		this.invLoc = invLoc;
		this.status = status;
	}

	public Long getTagId() {
		return tagId;
	}

	public void setTagId(Long tagId) {
		this.tagId = tagId;
	}

	public String getCompany() {
		return company;
	}

	public void setCompany(String company) {
		this.company = company;
	}
	public String getItemCode() {
		return itemCode;
	}

	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}

	public String getInvLoc() {
		return invLoc;
	}


	public void setInvLoc(String invLoc) {
		this.invLoc = invLoc;
	}


	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

}
