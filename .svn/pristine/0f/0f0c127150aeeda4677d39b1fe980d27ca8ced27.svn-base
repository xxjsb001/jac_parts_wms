$库位锁集 = 多值查表("R101_基础数据_库位出入锁表",出入锁类型)
每个($库位锁:$库位锁集){
    $区区间集=分解列表($库位锁.区区间)
    $区区间对象=赋值($区区间集,"开始值","结束值")	
    $排区间集=分解列表($库位锁.排区间)
    $排区间对象=赋值($排区间集,"开始值","结束值")	
    $列区间集=分解列表($库位锁.列区间)
    $列区间对象=赋值($列区间集,"开始值","结束值")	
    $层区间集=分解列表($库位锁.层区间)
    $层区间对象=赋值($层区间集,"开始值","结束值")	
	如果($库位锁.库区 == 库区 && $区区间对象.开始值<=区 && 区<=$区区间对象.结束值 && $排区间对象.开始值 <=排 && 排<=$排区间对象.结束值 && $列区间对象.开始值<=列 && 列<=$列区间对象.结束值 && $层区间对象.开始值<=层 && 层<= $层区间对象.结束值){
		异常("当前库位已被" + 出入锁类型 + "锁定!")
    }
}