$装车单波次数据集  = 数据查询("R101_装车单波次数据源", "仓库ID", 仓库ID)
如果($装车单波次数据集 != 空) {
	如果(列表大小($装车单波次数据集.装车单) > 0) {
		每个($装车单 : $装车单波次数据集.装车单) {
			$波次 = 创建对象()
			$波次.相关单号 = $装车单.装车单号
			$波次.波次结束时间 = $装车单.预计发车时间
			$波次.是否匹配已有波次 = "是"
			$波次.移位单列表 = 创建列表()
			每个($移位单 : $装车单.移位单) {
				加入列表($波次.移位单列表, $移位单 .序号)
			}
			加入列表(波次列表, $波次)
		}
	}
}