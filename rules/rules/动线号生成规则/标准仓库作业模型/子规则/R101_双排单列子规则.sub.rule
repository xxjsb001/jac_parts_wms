如果($区排所有列结果集 != 空) {
	如果(列表大小($区排所有列结果集) != 0) {
		列表对象排序($区排所有列结果集, "列号", $列排序方式)
		每个($排 : $区排所有列结果集) {            
			$排列 = 创建对象()
			$排列.排 = $奇排排号
			$排列.列 = $排.列号

			$排列.值 = 开始动线号
			加入列表(返回列表, $排列)	
			
			开始动线号 = 开始动线号 + 1

			$排列 = 创建对象()
			$排列.排 = $区.排号
			$排列.列 = $排.列号

			$排列.值 = 开始动线号
			加入列表(返回列表, $排列)

			开始动线号 = 开始动线号 + 1
			
		}
	}
}
