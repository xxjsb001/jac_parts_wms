费用 = 0
$费率=0
如果(作业人员 != 空) {
    $费率表 = 查表("R101_计费_计件工资费率表",货主,货品分类,类型)
	如果($费率表 == 空){
		$费率表 = 查表("R101_计费_计件工资费率表",货主,"所有",类型)
	}
	如果($费率表 == 空){
		$费率表 = 查表("R101_计费_计件工资费率表","所有","所有",类型)
	}
	如果($费率表 != 空){
		$计费量=件数
		如果($费率表.计费依据 == "重"){
			$计费量=重量
		}
		如果($费率表.计费依据 == "体"){
			$计费量=体积
		}
		$费率=$费率表 .费率
		费用 = $计费量 * $费率
	}
	
}
费率 = $费率
